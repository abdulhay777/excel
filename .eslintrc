{
  "parser": "babel-eslint",
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "rules": {
    "semi": "off",
    "arrow-parens": "off",
    "comma-dangle": "off",
    "linebreak-style": "off",
    "operator-linebreak": "off",
    "eol-last": "off",
    "require-jsdoc": "off",
    "quotes": "off",
    "max-len": "off",
    "indent": ["error", 2]
  },
  "extends": ["eslint:recommended", "google"]
}